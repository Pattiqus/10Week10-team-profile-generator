  "description": "Create HTML file with which to view team profiles based upon user input, testing with jest",
  "main": "src/index.js",
  "scripts": {
    "serve": "webpack serve",
    "start": "npm run copy-files && npm run build && node dist/index.js",
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
