  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pattiqus/10Week10-team-profile-generator/issues"
  },
  "homepage": "https://github.com/Pattiqus/10Week10-team-profile-generator#readme",
  "dependencies": {
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
